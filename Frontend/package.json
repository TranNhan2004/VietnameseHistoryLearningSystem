  "scripts": {
    "dev-all": "concurrently \"npm run dev-admin\" \"npm run dev-learner\"",
    "dev-admin": "npx nx serve admin --host 0.0.0.0 --port 4201 --allowed-hosts=admin-vhl.trannhanweb.io.vn",
    "dev-learner": "npx nx serve learner --host 0.0.0.0 --port 4202 --allowed-hosts=learner-vhl.trannhanweb.io.vn",
